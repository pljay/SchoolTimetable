package cn.wizool.schooltimetable.bean;

import java.util.ArrayList;
import java.util.List;

/**
 * 普通排课-任课明细
 * @author zl
 *
 */
public class CommonClass {

	/**
	 * 虚拟ID,仅在运算中使用。
	 */
	public int VirtualId=0;
	/**
	 * 原课程数据ID
	 */
	public String recordId="";
	/**
	 * 科目ID
	 */
	public String subjectId="";
	/**
	 * 科目名称
	 */
	public String subjectName="";
	/**
	 * 是否走班，1普通，2走班，默认1
	 */
	public int classType=1;
	/**
	 * 年级ID
	 */
	public String gradeID="";
	/**
	 * 班级ID
	 */
	public String classId="";
	/**
	 * 课程名称
	 */
	public String curriculumName="";
	/**
	 * 任课教师，允许多个
	 */
	public List<String> teachers=new ArrayList<String>();
	/**
	 * 所属周次，1为单周，2为双周
	 */
	public int Week=1;
	
	/**
	 * 单节上课次数
	 */
	public int singleClassTimes=0;
	/**
	 * 2节次连堂上课次数
	 */
	public int doubleClassTimes=0;
	/**
	 * 总节次数
	 */
	public int classCount=0;
	/**
	 * 早自习次数
	 */
	public int morningSlef=0;
	/**
	 * 晚自习次数
	 */
	public int nightSlef=0;
		
	/**
	 * 任务ID
	 */
	public String taskId="";
	/**
	 * 学校ID
	 */
	public String schoolId="";
	

}
